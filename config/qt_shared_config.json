{
    "qt_shared_configuration": {
        "version": "2.0",
        "description": "Qt库共享配置，用于PAGViewer与插件间的Qt库复用和简化部署",
        "qt_version": "6.8.1",
        "host_application": {
            "name": "PAGViewer",
            "bundle_id": "io.pag.pagviewer",
            "path": "/Applications/PAGViewer.app",
            "frameworks_directory": "Contents/Frameworks",
            "qt_modules": [
                "Qt3DAnimation",
                "Qt3DCore",
                "Qt3DExtras",
                "Qt3DInput",
                "Qt3DLogic",
                "Qt3DQuick",
                "Qt3DQuickScene2D",
                "Qt3DQuickScene3D",
                "Qt3DRender",
                "QtConcurrent",
                "QtCore",
                "QtDBus",
                "QtGui",
                "QtLabsPlatform",
                "QtLabsQmlModels",
                "QtLabsSettings",
                "QtNetwork",
                "QtOpenGL",
                "QtQml",
                "QtQmlLocalStorage",
                "QtQmlMeta",
                "QtQmlModels",
                "QtQmlWorkerScript",
                "QtQmlXmlListModel",
                "QtQuick",
                "QtQuickControls2",
                "QtQuickControls2Basic",
                "QtQuickControls2BasicStyleImpl",
                "QtQuickControls2FluentWinUI3StyleImpl",
                "QtQuickControls2Fusion",
                "QtQuickControls2FusionStyleImpl",
                "QtQuickControls2IOSStyleImpl",
                "QtQuickControls2Imagine",
                "QtQuickControls2ImagineStyleImpl",
                "QtQuickControls2Impl",
                "QtQuickControls2MacOSStyleImpl",
                "QtQuickControls2Material",
                "QtQuickControls2MaterialStyleImpl",
                "QtQuickControls2Universal",
                "QtQuickControls2UniversalStyleImpl",
                "QtQuickDialogs2",
                "QtQuickDialogs2QuickImpl",
                "QtQuickDialogs2Utils",
                "QtQuickEffects",
                "QtQuickLayouts",
                "QtQuickParticles",
                "QtQuickShapes",
                "QtQuickTemplates2",
                "QtQuickTimeline",
                "QtQuickTimelineBlendTrees",
                "QtQuickVectorImage",
                "QtQuickVectorImageGenerator",
                "QtQuickWidgets",
                "QtShaderTools",
                "QtSql",
                "QtStateMachine",
                "QtStateMachineQml",
                "QtSvg",
                "QtVirtualKeyboard",
                "QtVirtualKeyboardSettings",
                "QtWidgets"
            ]
        },
        "plugins": {
            "PAGExporter": {
                "type": "plugin",
                "bundle_extension": "plugin",
                "install_path": "/Library/Application Support/Adobe/Common/Plug-ins/7.0/MediaCore/PAGExporter.plugin",
                "qt_dependencies": [
                    "QtCore",
                    "QtGui",
                    "QtWidgets",
                    "QtOpenGL",
                    "QtQml",
                    "QtQuick",
                    "QtNetwork"
                ],
                "shared_library_mode": "symbolic_link",
                "backup_mode": "copy"
            }
        },
        "deployment": {
            "strategy": "symbolic_link_first",
            "fallback_to_copy": true,
            "create_backup": true,
            "log_level": "debug",
            "verify_integrity": true
        },
        "paths": {
            "log_directory": "/tmp/qt_shared_logs",
            "backup_directory": "/tmp/qt_shared_backup",
            "temp_directory": "/tmp/qt_shared_temp"
        }
    }
}