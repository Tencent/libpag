
gl::%t operator+(const gl::%t & a, std::underlying_type<gl::%t>::type b)
{
    return static_cast<gl::%t>(static_cast<std::underlying_type<gl::%t>::type>(a) + b);
}

gl::%t operator-(const gl::%t & a, std::underlying_type<gl::%t>::type b)
{
    return static_cast<gl::%t>(static_cast<std::underlying_type<gl::%t>::type>(a) - b);
}
