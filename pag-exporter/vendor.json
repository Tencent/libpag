    {
      "name": "AE_SDK",
      "dir": "vendor/AE_SDK",
      "cmake": {
        "targets": [
          "ae_sdk_util"
        ],
        "platforms": [
          "mac",
          "win"
        ]
      }
    },
    {
      "name": "ffmpeg",
