<?xml version="1.0" encoding="UTF-8"?>
<pagx version="1.0" width="800" height="600">
  <Resources>
    <!-- Background Gradient: Deep Space -->
    <LinearGradient id="bgGradient" startPoint="0,0" endPoint="800,600">
      <ColorStop offset="0" color="#050A14"/>
      <ColorStop offset="0.5" color="#0B1221"/>
      <ColorStop offset="1" color="#1A1025"/>
    </LinearGradient>

    <!-- Glassmorphism Card Background -->
    <LinearGradient id="glassFill" startPoint="0,0" endPoint="0,100">
      <ColorStop offset="0" color="#FFFFFF10"/>
      <ColorStop offset="1" color="#FFFFFF05"/>
    </LinearGradient>

    <!-- Neon Accents -->
    <LinearGradient id="accentCyan" startPoint="0,0" endPoint="100,0">
      <ColorStop offset="0" color="#00F2FF"/>
      <ColorStop offset="1" color="#00A8FF"/>
    </LinearGradient>
    <LinearGradient id="accentPurple" startPoint="0,0" endPoint="100,0">
      <ColorStop offset="0" color="#BD00FF"/>
      <ColorStop offset="1" color="#7000FF"/>
    </LinearGradient>
    <LinearGradient id="accentOrange" startPoint="0,0" endPoint="100,0">
      <ColorStop offset="0" color="#FF9900"/>
      <ColorStop offset="1" color="#FF5500"/>
    </LinearGradient>
    <LinearGradient id="accentGreen" startPoint="0,0" endPoint="100,0">
      <ColorStop offset="0" color="#00FF9D"/>
      <ColorStop offset="1" color="#00CC6A"/>
    </LinearGradient>
    <LinearGradient id="accentPink" startPoint="0,0" endPoint="100,0">
      <ColorStop offset="0" color="#FF0080"/>
      <ColorStop offset="1" color="#FF0055"/>
    </LinearGradient>

    <!-- Abstract Background Shape -->
    <PathData id="gridPattern" data="M 0 0 H 800 M 0 50 H 800 M 0 100 H 800 M 0 150 H 800 M 0 200 H 800 M 0 250 H 800 M 0 300 H 800 M 0 350 H 800 M 0 400 H 800 M 0 450 H 800 M 0 500 H 800 M 0 550 H 800 M 0 0 V 600 M 50 0 V 600 M 100 0 V 600 M 150 0 V 600 M 200 0 V 600 M 250 0 V 600 M 300 0 V 600 M 350 0 V 600 M 400 0 V 600 M 450 0 V 600 M 500 0 V 600 M 550 0 V 600 M 600 0 V 600 M 650 0 V 600 M 700 0 V 600 M 750 0 V 600"/>

    <!-- Icon: Open (Code Brackets + Eye) -->
    <PathData id="iconOpen" data="M 10 20 L 25 5 L 25 15 L 45 15 L 45 5 L 60 20 L 45 35 L 45 25 L 25 25 L 25 35 Z M 35 38 A 4 4 0 1 1 35 46 A 4 4 0 1 1 35 38"/>
    
    <!-- Icon: Feature (Layers Stack) -->
    <PathData id="iconFeature" data="M 10 25 L 35 15 L 60 25 L 35 35 Z M 10 35 L 35 45 L 60 35 M 10 45 L 35 55 L 60 45"/>

    <!-- Icon: Concise (Minimize/Zip) -->
    <PathData id="iconConcise" data="M 20 10 L 50 10 L 50 20 L 40 20 L 40 30 L 30 30 L 30 20 L 20 20 Z M 30 30 L 40 30 L 40 40 L 30 40 Z M 20 50 L 50 50 L 50 40 L 20 40 Z"/>

    <!-- Icon: Ecosystem (Network) -->
    <PathData id="iconEco" data="M 35 10 A 5 5 0 1 1 35 20 A 5 5 0 1 1 35 10 M 15 40 A 5 5 0 1 1 15 50 A 5 5 0 1 1 15 40 M 55 40 A 5 5 0 1 1 55 50 A 5 5 0 1 1 55 40 M 35 20 L 18 42 M 35 20 L 52 42 M 20 45 L 50 45"/>

    <!-- Icon: Deployment (Rocket/Launch) -->
    <PathData id="iconDeploy" data="M 35 5 C 35 5 15 35 15 50 L 25 50 L 25 60 L 35 55 L 45 60 L 45 50 L 55 50 C 55 35 35 5 35 5"/>

  </Resources>

  <!-- Main Background -->
  <Layer name="Background">
    <Rectangle center="400,300" size="800,600"/>
    <Fill color="@bgGradient"/>
  </Layer>

  <!-- Grid Pattern (Low Opacity) -->
  <Layer name="Grid" alpha="0.05">
    <Path data="@gridPattern"/>
    <Stroke color="#FFFFFF" width="1"/>
  </Layer>

  <!-- Decorative Orbs -->
  <Layer name="Orb1" x="100" y="500" alpha="0.3" blendMode="screen">
    <Ellipse center="0,0" size="400,400"/>
    <Fill>
      <RadialGradient center="0,0" radius="200">
        <ColorStop offset="0" color="#7000FF"/>
        <ColorStop offset="1" color="#00000000"/>
      </RadialGradient>
    </Fill>
  </Layer>
  <Layer name="Orb2" x="700" y="100" alpha="0.2" blendMode="screen">
    <Ellipse center="0,0" size="500,500"/>
    <Fill>
      <RadialGradient center="0,0" radius="250">
        <ColorStop offset="0" color="#00A8FF"/>
        <ColorStop offset="1" color="#00000000"/>
      </RadialGradient>
    </Fill>
  </Layer>

  <!-- Title Section -->
  <Layer name="Header" y="60">
    <!-- Main Title -->
    <Text text="PAGX" fontFamily="Arial" fontStyle="Bold" fontSize="72"/>
    <TextLayout position="400,0" textAlign="center"/>
    <Fill>
      <LinearGradient startPoint="300,0" endPoint="500,0">
        <ColorStop offset="0" color="#FFFFFF"/>
        <ColorStop offset="1" color="#AAAAAA"/>
      </LinearGradient>
    </Fill>
    <DropShadowStyle offsetX="0" offsetY="4" blurX="10" blurY="10" color="#00000080"/>
    
    <!-- Subtitle -->
    <Layer y="50">
        <Text text="The Future of Animated Graphics" fontFamily="Arial" fontSize="18" letterSpacing="2"/>
        <TextLayout position="400,0" textAlign="center"/>
        <Fill color="#FFFFFF80"/>
    </Layer>
  </Layer>

  <!-- Layout: 3 Top, 2 Bottom -->
  
    <!-- Card 1: Open & Readable -->
  <Layer name="Card1" x="50" y="160">
    <DropShadowStyle offsetX="0" offsetY="8" blurX="20" blurY="20" color="#00000060"/>
    
    <!-- Card Background -->
    <Group>
        <Rectangle center="110,75" size="220,150" roundness="16"/>
        <Fill color="@glassFill"/>
        <Stroke width="1.5">
            <LinearGradient startPoint="0,0" endPoint="220,150">
                <ColorStop offset="0" color="#00F2FF40"/>
                <ColorStop offset="1" color="#00F2FF10"/>
            </LinearGradient>
        </Stroke>
    </Group>

    <!-- Icon Circle -->
    <Group position="30, 30">
        <Ellipse center="0,0" size="44,44"/>
        <Fill color="#00F2FF20"/>
        <Stroke color="#00F2FF" width="1"/>
    </Group>
    <Group position="-5, -5">
        <Path data="@iconOpen"/>
        <Fill color="@accentCyan"/>
    </Group>

    <!-- Title -->
    <Group position="20, 70">
        <Text text="Open &amp; Readable" fontFamily="Arial" fontStyle="Bold" fontSize="18"/>
        <Fill color="#FFFFFF"/>
    </Group>

    <!-- Body Text: Manual wrapping -->
    <Group position="20, 100">
        <Text text="XML-based plain text format." position="0,0" fontFamily="Arial" fontSize="12"/>
        <Text text="Native version control friendly." position="0,18" fontFamily="Arial" fontSize="12"/>
        <Text text="Optimized for AI generation." position="0,36" fontFamily="Arial" fontSize="12"/>
        <Fill color="#FFFFFFCC"/>
    </Group>
  </Layer>

  <!-- Card 2: Feature Complete -->
  <Layer name="Card2" x="290" y="160">
    <DropShadowStyle offsetX="0" offsetY="8" blurX="20" blurY="20" color="#00000060"/>
    <Group>
        <Rectangle center="110,75" size="220,150" roundness="16"/>
        <Fill color="@glassFill"/>
        <Stroke width="1.5">
            <LinearGradient startPoint="0,0" endPoint="220,150">
                <ColorStop offset="0" color="#BD00FF40"/>
                <ColorStop offset="1" color="#BD00FF10"/>
            </LinearGradient>
        </Stroke>
    </Group>
    <Group position="30, 30">
        <Ellipse center="0,0" size="44,44"/>
        <Fill color="#BD00FF20"/>
        <Stroke color="#BD00FF" width="1"/>
    </Group>
    <Group position="-5, -5">
        <Path data="@iconFeature"/>
        <Fill color="@accentPurple"/>
    </Group>
    <Group position="20, 70">
        <Text text="Feature Complete" fontFamily="Arial" fontStyle="Bold" fontSize="18"/>
        <Fill color="#FFFFFF"/>
    </Group>
    <!-- Body Text: Manual wrapping -->
    <Group position="20, 100">
        <Text text="Vectors, images, &amp; rich text." position="0,0" fontFamily="Arial" fontSize="12"/>
        <Text text="Advanced filters &amp; blending." position="0,18" fontFamily="Arial" fontSize="12"/>
        <Text text="Supports complex motion." position="0,36" fontFamily="Arial" fontSize="12"/>
        <Fill color="#FFFFFFCC"/>
    </Group>
  </Layer>

  <!-- Card 3: Concise -->
  <Layer name="Card3" x="530" y="160">
    <DropShadowStyle offsetX="0" offsetY="8" blurX="20" blurY="20" color="#00000060"/>
    <Group>
        <Rectangle center="110,75" size="220,150" roundness="16"/>
        <Fill color="@glassFill"/>
        <Stroke width="1.5">
            <LinearGradient startPoint="0,0" endPoint="220,150">
                <ColorStop offset="0" color="#FF990040"/>
                <ColorStop offset="1" color="#FF990010"/>
            </LinearGradient>
        </Stroke>
    </Group>
    <Group position="30, 30">
        <Ellipse center="0,0" size="44,44"/>
        <Fill color="#FF990020"/>
        <Stroke color="#FF9900" width="1"/>
    </Group>
    <Group position="-5, -5">
        <Path data="@iconConcise"/>
        <Fill color="@accentOrange"/>
    </Group>
    <Group position="20, 70">
        <Text text="Concise &amp; Efficient" fontFamily="Arial" fontStyle="Bold" fontSize="18"/>
        <Fill color="#FFFFFF"/>
    </Group>
    <!-- Body Text: Manual wrapping -->
    <Group position="20, 100">
        <Text text="Compact unified structure." position="0,0" fontFamily="Arial" fontSize="12"/>
        <Text text="Efficient binary conversion." position="0,18" fontFamily="Arial" fontSize="12"/>
        <Text text="Extensible architecture." position="0,36" fontFamily="Arial" fontSize="12"/>
        <Fill color="#FFFFFFCC"/>
    </Group>
  </Layer>

  <!-- Card 4: Ecosystem (Bottom Left, wider) -->
  <Layer name="Card4" x="110" y="340">
    <DropShadowStyle offsetX="0" offsetY="8" blurX="20" blurY="20" color="#00000060"/>
    <Group>
        <Rectangle center="140,75" size="280,150" roundness="16"/>
        <Fill color="@glassFill"/>
        <Stroke width="1.5">
            <LinearGradient startPoint="0,0" endPoint="280,150">
                <ColorStop offset="0" color="#00FF9D40"/>
                <ColorStop offset="1" color="#00FF9D10"/>
            </LinearGradient>
        </Stroke>
    </Group>
    <Group position="30, 30">
        <Ellipse center="0,0" size="44,44"/>
        <Fill color="#00FF9D20"/>
        <Stroke color="#00FF9D" width="1"/>
    </Group>
    <Group position="-5, -5">
        <Path data="@iconEco"/>
        <Fill color="@accentGreen"/>
    </Group>
    <Group position="20, 70">
        <Text text="Ecosystem Compatible" fontFamily="Arial" fontStyle="Bold" fontSize="18"/>
        <Fill color="#FFFFFF"/>
    </Group>
    <!-- Body Text: Manual wrapping -->
    <Group position="20, 100">
        <Text text="Universal interchange format." position="0,0" fontFamily="Arial" fontSize="12"/>
        <Text text="Supports AE, Figma, &amp; more." position="0,18" fontFamily="Arial" fontSize="12"/>
        <Text text="Seamless asset workflow." position="0,36" fontFamily="Arial" fontSize="12"/>
        <Fill color="#FFFFFFCC"/>
    </Group>
  </Layer>

  <!-- Card 5: Deployment (Bottom Right, wider) -->
  <Layer name="Card5" x="410" y="340">
    <DropShadowStyle offsetX="0" offsetY="8" blurX="20" blurY="20" color="#00000060"/>
    <Group>
        <Rectangle center="140,75" size="280,150" roundness="16"/>
        <Fill color="@glassFill"/>
        <Stroke width="1.5">
            <LinearGradient startPoint="0,0" endPoint="280,150">
                <ColorStop offset="0" color="#FF008040"/>
                <ColorStop offset="1" color="#FF008010"/>
            </LinearGradient>
        </Stroke>
    </Group>
    <Group position="30, 30">
        <Ellipse center="0,0" size="44,44"/>
        <Fill color="#FF008020"/>
        <Stroke color="#FF0080" width="1"/>
    </Group>
    <Group position="-5, -5">
        <Path data="@iconDeploy"/>
        <Fill color="@accentPink"/>
    </Group>
    <Group position="20, 70">
        <Text text="Efficient Deployment" fontFamily="Arial" fontStyle="Bold" fontSize="18"/>
        <Fill color="#FFFFFF"/>
    </Group>
    <!-- Body Text: Manual wrapping -->
    <Group position="20, 100">
        <Text text="One-click production export." position="0,0" fontFamily="Arial" fontSize="12"/>
        <Text text="High compression ratio." position="0,18" fontFamily="Arial" fontSize="12"/>
        <Text text="High performance runtime." position="0,36" fontFamily="Arial" fontSize="12"/>
        <Fill color="#FFFFFFCC"/>
    </Group>
  </Layer>

  <!-- Footer -->
  <Layer name="Footer" y="550">
    <Text text="Generated with PAGX" fontFamily="Arial" fontSize="12" letterSpacing="1"/>
    <TextLayout position="400,0" textAlign="center"/>
    <Fill color="#FFFFFF40"/>
  </Layer>

</pagx>
