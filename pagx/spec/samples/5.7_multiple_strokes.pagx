<?xml version="1.0" encoding="UTF-8"?>
<!-- Demonstrates multiple Stroke layers creating outline effect -->
<pagx version="1.0" width="200" height="200">
  <Layer>
    <Path data="M 30,100 Q 100,30 170,100 T 170,170"/>
    <Stroke color="#3498DB20" width="16" cap="round" join="round"/>
    <Stroke color="#3498DB60" width="8" cap="round" join="round"/>
    <Stroke color="#3498DB" width="3" cap="round" join="round"/>
  </Layer>
</pagx>
