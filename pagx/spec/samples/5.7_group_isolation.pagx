<?xml version="1.0" encoding="UTF-8"?>
<!-- Demonstrates Group isolation: alpha applied to group, not individual shapes -->
<pagx version="1.0" width="200" height="200">
  <Layer>
    <Group alpha="0.6">
      <Rectangle center="70,100" size="100,100" roundness="8"/>
      <Fill color="#FF6B6B"/>
    </Group>
    <Ellipse center="130,100" size="100,100"/>
    <Fill color="#3498DB"/>
  </Layer>
</pagx>
